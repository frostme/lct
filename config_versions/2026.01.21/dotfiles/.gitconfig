[user]
	email = elliot@rocketmoney.com
  name = M. Elliot Frost
	signingkey = /Users/EFrost/.ssh/id_ed25519.pub
[url "git@github.com:"]
	insteadOf = https://github.com/
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/Sourcetree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[commit]
  template = /Users/elliotfrost/.stCommitMsg
	gpgsign = true
[gpg]
	format = ssh
[init]
	defaultBranch = main
